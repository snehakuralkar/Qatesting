package com.tcs.delta.billing;

import java.util.Scanner;

public class Hello {
 public static void main(String[] args) {
	 int n1 = 0, n2 = 1, num;
     System.out.print(n1 + " " +
             n2 + " ");

     for (int i = 0; i < 100; i++) {
         num = n2 + n1;
         System.out.print(num + " ");
         n1 = n2;
         n2 = num;
     
 }
}
}